{
  "name": "discord-music-bot",
  "version": "2.0.0",
  "description": "Professional Discord Music Bot with CommandKit and Distube",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "bot": "node start.js",
    "docker:build": "docker build -t discord-music-bot .",
    "docker:run": "docker run -d --name discord-music-bot discord-music-bot",
    "docker:compose": "docker-compose up -d",
    "docker:logs": "docker-compose logs -f",
    "docker:stop": "docker-compose down",
    "test": "node test.js",
    "prod": "node production.js"
  },
  "keywords": ["discord", "music", "bot", "discord.js", "commandkit", "distube"],
  "author": "Professional Developer",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "distube": "^4.1.1",
    "@distube/spotify": "^1.5.1",
    "@distube/soundcloud": "^1.3.3",
    "@distube/yt-dlp": "^1.1.4",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.1",
    "ffmpeg-static": "^5.2.0",
    "libsodium-wrappers": "^0.7.13",
    "sodium": "^3.0.2",
    "ytdl-core": "^4.11.5",
    "play-dl": "^1.9.7",
    "spotify-web-api-node": "^5.0.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}